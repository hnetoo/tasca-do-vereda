{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Tasca Do VEREDA - Gest√£o Inteligente",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 800,
        "minHeight": 600,
        "dragDropEnabled": false,
        "dataDirectory": "stable-v1.1.70-fixed",
        "devtools": true,
        "additionalBrowserArgs": "--disable-gpu --disable-software-rasterizer --disable-dev-shm-usage --disable-features=RendererCodeIntegrity"
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["default"]
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "msi"
    ],
    "windows": {
      "webviewInstallMode": {
        "type": "embedBootstrapper",
        "silent": true
      }
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Tasca Do VEREDA",
  "version": "1.1.70",
  "identifier": "com.tasca-do-vereda.client"
}
